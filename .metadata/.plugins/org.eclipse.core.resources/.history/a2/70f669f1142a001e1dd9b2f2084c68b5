package com.corejava.amit;
import java.util.*;
public class DaysInBetween {
	

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int dd1,mm1,yy1,dd2,mm2,yy2,days=0;
		int dm;
		System.out.println("dd1 :");
		dd1=sc.nextInt();
		System.out.println("mm1 :");
		mm1=sc.nextInt();
		System.out.println("yy1 :");
		yy1=sc.nextInt();
		System.out.println("dd2 :");
		dd2=sc.nextInt();
		System.out.println("mm2 :");
		mm2=sc.nextInt();
		System.out.println("yy2 :");
		yy2=sc.nextInt();
		
		//if yy1==yy2
		if(yy1==yy2) {
				if(mm1<mm2) {
					for(int i=mm1;i<=mm2;i++) {
						if((yy1/4==0&&yy1/100!=0)||yy1/400==0) {
							switch(i) {
							case 1:
							case 3:
							case 5:
							case 7:
							case 8:
							case 10:
							case 12:
								dm=31;
								break;
							case 2:
								dm=29;
								break;
							default :
								dm=30;			
							}
						}
			
						else {
							switch(i) {
							case 1:
							case 3:
							case 5:
							case 7:
							case 8:
							case 10:
							case 12:
								dm=31;
								break;
							case 2:
								dm=28;
								break;
							default :
								dm=30;			
							}
						}
						
						if(i==mm1) {
							days+=(dm-dd1);
						}
						else if(i==mm2) {
							days+=dd2;
						}
						else
							days+=dm;
					}
				}
				if(mm1>mm2) {
					for(int i=mm2;i<=mm1;i++) {
						if((yy1/4==0&&yy1/100!=0)||yy1/400==0) {
							switch(i) {
							case 1:
							case 3:
							case 5:
							case 7:
							case 8:
							case 10:
							case 12:
								dm=31;
								break;
							case 2:
								dm=29;
								break;
							default :
								dm=30;			
							}
						}
						else {
							switch(i) {
							case 1:
							case 3:
							case 5:
							case 7:
							case 8:
							case 10:
							case 12:
								dm=31;
								break;
							case 2:
								dm=28;
								break;
							default :
								dm=30;			
							}
						}
						if(i==mm2) {
							days+=(dm-dd2);
						}
						else if(i==mm1) {
							days+=dd1;
						}
						else
							days+=dm;
					}
				}
				if(mm1==mm2)
					days=dd1-dd2;
					days=Math.abs(days);
				
		}
		
		
		//if yy1<yy2
		if(yy1<yy2) {
			
		for(int i=yy1;i<=yy2;i++) {
					if(i==yy1) {
						for(int j = mm1;j<=12;j++) {
							if((i/4==0&&i/100!=0)||i/400==0) {
								switch(j) {
								case 1:
								case 3:
								case 5:
								case 7:
								case 8:
								case 10:
								case 12:
									dm=31;
									break;
								case 2:
									dm=29;
									break;
								default :
									dm=30;			
								}
							}
							else {
								switch(j) {
								case 1:
								case 3:
								case 5:
								case 7:
								case 8:
								case 10:
								case 12:
									dm=31;
									break;
								case 2:
									dm=28;
									break;
								default :
									dm=30;			
								}
							}
							if(i==yy1&&j==mm1)
								days+=(dm-dd1)+1;
							else 
								days+=dm;
					}
					
				}
				else if(i==yy2) {
					for(int j = 1;j<=mm2;j++) {
						if((i/4==0&&i/100!=0)||i/400==0) {
							switch(j) {
							case 1:
							case 3:
							case 5:
							case 7:
							case 8:
							case 10:
							case 12:
								dm=31;
								break;
							case 2:
								dm=29;
								break;
							default :
								dm=30;			
							}
						}
						else {
							switch(j) {
							case 1:
							case 3:
							case 5:
							case 7:
							case 8:
							case 10:
							case 12:
								dm=31;
								break;
							case 2:
								dm=28;
								break;
							default :
								dm=30;			
							}
						}
						if(i==yy2&&j==mm2)
							days+=dd2;
						else 
							days+=dm;
					}
				}
				else {
					for(int j = 1;j<=12;j++) {
						if((i/4==0&&i/100!=0)||i/400==0) {
							switch(j) {
							case 1:
							case 3:
							case 5:
							case 7:
							case 8:
							case 10:
							case 12:
								dm=31;
								break;
							case 2:
								dm=29;
								break;
							default :
								dm=30;			
							}
						}
						else {
							switch(j) {
							case 1:
							case 3:
							case 5:
							case 7:
							case 8:
							case 10:
							case 12:
								dm=31;
								break;
							case 2:
								dm=28;
								break;
							default :
								dm=30;			
							}
						}
						days+=dm;
					}
				}
			}
		}
		
		//if yy1>yy2
				if(yy1>yy2) {
					
				for(int i=yy2;i<=yy1;i++) {
							if(i==yy2) {
								for(int j = mm1;j<=12;j++) {
									if((i/4==0&&i/100!=0)||i/400==0) {
										switch(j) {
										case 1:
										case 3:
										case 5:
										case 7:
										case 8:
										case 10:
										case 12:
											dm=31;
											break;
										case 2:
											dm=29;
											break;
										default :
											dm=30;			
										}
									}
									else {
										switch(j) {
										case 1:
										case 3:
										case 5:
										case 7:
										case 8:
										case 10:
										case 12:
											dm=31;
											break;
										case 2:
											dm=28;
											break;
										default :
											dm=30;			
										}
									}
									if(i==yy2&&j==mm2)
										days+=(dm-dd2)+1;
									else 
										days+=dm;
							}
							
						}
						else if(i==yy1) {
							for(int j = 1;j<=mm1;j++) {
								if((i/4==0&&i/100!=0)||i/400==0) {
									switch(j) {
									case 1:
									case 3:
									case 5:
									case 7:
									case 8:
									case 10:
									case 12:
										dm=31;
										break;
									case 2:
										dm=29;
										break;
									default :
										dm=30;			
									}
								}
								else {
									switch(j) {
									case 1:
									case 3:
									case 5:
									case 7:
									case 8:
									case 10:
									case 12:
										dm=31;
										break;
									case 2:
										dm=28;
										break;
									default :
										dm=30;			
									}
								}
								if(i==yy1&&j==mm1)
									days+=dd1;
								else 
									days+=dm;
							}
						}
						else {
							for(int j = 1;j<=12;j++) {
								if((i/4==0&&i/100!=0)||i/400==0) {
									switch(j) {
									case 1:
									case 3:
									case 5:
									case 7:
									case 8:
									case 10:
									case 12:
										dm=31;
										break;
									case 2:
										dm=29;
										break;
									default :
										dm=30;			
									}
								}
								else {
									switch(j) {
									case 1:
									case 3:
									case 5:
									case 7:
									case 8:
									case 10:
									case 12:
										dm=31;
										break;
									case 2:
										dm=28;
										break;
									default :
										dm=30;			
									}
								}
								days+=dm;
						}
							
					}
				}
				
			}
		System.out.println("Days in between : "+days);
	}
}
