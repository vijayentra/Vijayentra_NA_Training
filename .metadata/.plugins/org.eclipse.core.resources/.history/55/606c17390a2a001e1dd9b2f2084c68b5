package com.corejava.amit;
import java.util.*;
public class DaysInBetween {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int dd1,mm1,yy1,dd2,mm2,yy2,days=0;
		int dm;
		dd1=sc.nextInt();
		mm1=sc.nextInt();
		yy1=sc.nextInt();
		dd2=sc.nextInt();
		mm2=sc.nextInt();
		yy2=sc.nextInt();
		
		
		if(yy1<yy2) {
			
		for(int i=yy1;i<=yy2;i++) {
				for(int j = mm1;j<=12;j++) {
					if((i/4==0&&i/100!=0)||i/400==0) {
						switch(j) {
						case 1:
						case 3:
						case 5:
						case 7:
						case 8:
						case 10:
						case 12:
							dm=31;
							break;
						case 2:
							dm=28;
							break;
						default :
							dm=30;			
						}
					}
					else {
						switch(j) {
						case 1:
						case 3:
						case 5:
						case 7:
						case 8:
						case 10:
						case 12:
							dm=31;
							break;
						case 2:
							dm=29;
							break;
						default :
							dm=30;			
						}
					}
						
					if(i==yy1&&j==mm1)
						days+=(dm-dd1);
					else if(i==yy2&&j==mm2)
						days+=(dd2);
					else 
						days+=dm;
				}
			}
		}
	}

}
