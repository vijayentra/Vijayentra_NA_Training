package com.corejava.a1;
import java.util.*;
public class ArmstrongNumber {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int sum=0,r;
		while(n>0) {
			r=n%10;
			sum+=r*r*r;
			n=n/10;
		}
		if(sum==n)
			System.out.println("armstrong number");
		else
			System.out.println("not an armstrong number");
	}

}
