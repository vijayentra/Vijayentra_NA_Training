package com.corejava.a2;

public class Employee {
	String emp_name;
	double salary;
	
	public Employee(String emp_name,double salary) {
		this.emp_name=emp_name;
		this.salary=salary;
		}
	public double Salary() {
		return salary;
	}
	
	public String toString() {
        return emp_name + " (Employee)";
    }
	
	
}
