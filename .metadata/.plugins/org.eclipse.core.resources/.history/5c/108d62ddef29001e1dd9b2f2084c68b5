package com.corejava.a1;
import java.util.*;
public class ArmstrongNumber {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int sum=0,r,temp=n;
		while(temp>0) {
			r=temp%10;
			sum+=(r*r*r);
			temp=temp/10;
		}
		if(sum==n)
			System.out.println("armstrong number");
		else
			System.out.println("not an armstrong number");
	}

}
