class Practise{
	public static void main(String[] args) {
		String name = new String("na");
		
		String n = "na";
		String n2 = "na";
		if(n2==name) {
			System.out.println("hi");
		}
		else {
			System.out.println("bye");
		}
		System.out.println(name.toUpperCase());
		System.out.println(n.toUpperCase());
	}
}
